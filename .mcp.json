{
  "mcpServers": {
    "sentry": {
      "type": "http",
      "url": "https://mcp.sentry.dev/mcp"
    },
    "next-devtools": {
      "command": "npx",
      "args": ["-y", "next-devtools-mcp@latest"]
    },
    "google-analytics": {
      "command": "npx",
      "args": ["-y", "mcp-server-google-analytics"]
    },
    "expo-mcp": {
      "command": "npx",
      "args": ["-y", "expo-docs-mcp"]
    },
    "maestro": {
      "command": "npx",
      "args": ["-y", "@mobile-dev-inc/maestro-mcp"]
    }
  }
}
